{
  "compilerOptions": {
    "jsx": "react",
    "paths": {
      "@/components/*": [
        "src/components/*"
      ],
      "@/utils/*": [
        "./src/utils/*"
      ],
      "@/lib/*": [
        "./src/lib/*"
      ],
      "@/context/*": [
        "./src/context/*"
      ],
      "@/pages/*": [
        "src/pages/*"
      ],
      "@/styles/*": [
        "src/styles/*"
      ]
      // Add more paths as needed
    },
  }
}
